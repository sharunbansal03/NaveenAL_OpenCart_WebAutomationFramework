<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" thread-count="5">

<listeners>
<listener class-name="genericUtilities.ListenerImplementationClass"></listener>
</listeners>

  <test thread-count="5" name="Test_Firefox_Windows10">
  <parameter name="browserName" value="Firefox"/>
  <parameter name="browserVersion" value="117"/>
  <parameter name="platformName" value="Windows 10"/>
  
    <classes>
      <class name="MyAccount.Tests.LoginTest"/>
      <class name="MyAccount.Tests.LogOutTest"/>
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="Test_Firefox_Windows11">
  <parameter name="browserName" value="Firefox"/>
  <parameter name="browserVersion" value="latest"/>
  <parameter name="platformName" value="Windows 11"/>
  
    <classes>
      <class name="MyAccount.Tests.LoginTest"/>
      <class name="MyAccount.Tests.LogOutTest"/>
    </classes>
  </test> <!-- Test -->
  
</suite> <!-- Suite -->
